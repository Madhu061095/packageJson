  "private": true,
  "scripts": {
    "prepare": "husky install",
    "dev": "cross-env TS_NODE_PROJECT=\"tsconfig-for-webpack-config.json\" webpack-dev-server --config ./webpack.dev.ts",
    "lint": "eslint src/**/*.{ts,tsx}",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest",
    "test:coverage": "npm run test -- --coverage",
    "build": "cross-env TS_NODE_PROJECT=\"tsconfig-for-webpack-config.json\" webpack",
    "build-source-map": "cross-env TS_NODE_PROJECT=\"tsconfig-for-webpack-config.json\" webpack --devtool source-map",
    "analyze": "source-map-explorer './dist/*.js' --html tree.html"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
